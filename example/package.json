{
  "name": "example",
  "description": "A simple example package to validate the eslint plugin works as a devDependency to a project that uses TestCafe",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext ts,js .",
    "test": "testcafe chrome:headless **/*.test.ts **/*.test.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-testcafe-community": "file:eslint-plugin-testcafe-community-1.0.0-autoversioned.tgz",
    "testcafe": "^1.16.1"
  }
}
